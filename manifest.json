{
    "name": "Girls vs Git",
    "version": "0.1",
    "manifest_version": 2,
    "description": "A Girlier GitHub",
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*"
            ],
            "js": [
                "jquery.js",
                "cookies.js",
                "content_script.js"
            ]
        }
    ],
    "content_security_policy": "script-src 'self' http://localhost; object-src 'self' http://localhost",
    "incognito": "split",
    "offline_enabled": false,
    "permissions": [
        "tabs",
        "cookies",
        "https://*.github.com/*",
        "http://localhost/*",
        "https://graph.facebook.com/*",
        "http://profile.ak.fbcdn.net/*"
    ],
    "update_url": "http://path/to/updateInfo.xml",
    "web_accessible_resources": [
        "manifest.json",
        "common_first_names.txt",
        "common_last_names.txt",
        "usernames.txt"
    ]
}